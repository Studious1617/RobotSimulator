<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.RulesetsCreate">
   <children>
      <HBox prefHeight="50.0" prefWidth="1400.0" style="-fx-background-color: Gray;">
         <children>
            <Label text="Label" />
         </children>
      </HBox>
      <HBox layoutY="50.0" prefHeight="50.0" prefWidth="1400.0" style="-fx-background-color: Silver;">
         <children>
            <Label text="Label" />
         </children>
      </HBox>
      <GridPane layoutX="8.0" layoutY="100.0" prefHeight="150.0" prefWidth="1400.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="backButton" mnemonicParsing="false" onAction="#onBackButtonClick" text="Back" />
            <Button alignment="CENTER_RIGHT" mnemonicParsing="false" text="Save" GridPane.columnIndex="1" />
            <VBox prefHeight="200.0" prefWidth="100.0" GridPane.rowIndex="1">
               <children>
                  <Label text="Label" />
                  <Label minWidth="600.0" text="Label" />
               </children>
            </VBox>
         </children>
      </GridPane>
      <Pane layoutX="8.0" layoutY="230.0" prefHeight="200.0" prefWidth="1400.0">
         <children>
            <ScrollPane prefHeight="360.0" prefWidth="1400.0">
              <content>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="200.0" />
              </content>
            </ScrollPane>
         </children>
      </Pane>
   </children>
</AnchorPane>
